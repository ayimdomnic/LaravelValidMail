{
    "name": "ayim/laravel-valid-mail",
    "description": "A Laravel Package to validate valid email addresses",
    "type": "library",
    "license": "Apache2.0",
    "authors": [
        {
            "name": "Odhiambo Dorminic",
            "email": "ayimdomnic@gmail.com"
        }
    ],
    "require": {
        "php" : "^7.0",
        "illuminate/support": "^5.7"
    },

    "require-dev":{
        "orchestra/testbench": "^3.4",
        "phpunit/phpunit": "^7.2",
        "fzaninotto/faker": "^1.7"
    },

    "autoload": {
        "psr-4": {
            "Ayim\\LaravelValidMail\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Ayim\\LaravelValidMail\\Tests\\": "tests/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "Ayim\\LaravelValidMail\\LaravelValidMailServiceProvider"
            ]
        },
        "aliases": {
            "ValidMail": "Ayim\\LaravelValidMail\\Facades\\ValidMail"
        }
    }

}
